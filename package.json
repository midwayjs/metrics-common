{
  "name": "metrics-common",
  "version": "1.0.0",
  "main": "dist/index",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npm run lint && rm -rf ./dist && midway-bin build",
    "lint": "tslint --format prose -c ./tslint.json src/**/*.ts test/**/*.ts",
    "test": "npm run lint && midway-bin clean && midway-bin test --ts",
    "cov": "midway-bin cov --tes",
    "ci": "npm run test"
  },
  "keywords": [
    "metrics",
    "alimetrics"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/midwayjs/metrics-common.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/lodash": "^4.14.109",
    "@types/mocha": "^2.2.41",
    "@types/node": "^10.5.5",
    "chai": "^4.2.0",
    "git-hooks": "^1.1.10",
    "midway-bin": "^0.3.8",
    "ts-node": "^7.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.0"
  },
  "dependencies": {},
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "typings",
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "json",
      "html",
      "lcovonly"
    ],
    "all": true
  },
  "files": [
    "dist",
    "src"
  ]
}
